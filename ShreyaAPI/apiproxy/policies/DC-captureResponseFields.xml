<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataCapture name="DC-captureResponseFields" continueOnError="false" enabled="true">
    <DisplayName>DC-captureResponseFields</DisplayName>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Capture>
        <Collect ref="responseLogs.resp-errorCode"/>
        <DataCollector>dc_backend_error_code</DataCollector>
    </Capture>
    <Capture>
        <Collect ref="responseLogs.resp-errorMessage"/>
        <DataCollector>dc_backend_error_message</DataCollector>
    </Capture>
</DataCapture>